void setup() {
   pinMode(LED_BUILTIN, OUTPUT);

}

void loop() {
 //M

 digitalWrite(LED_BUILTIN, HIGH);   
  delay(600);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
   digitalWrite(LED_BUILTIN, HIGH);   
  delay(600);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, LOW);   
  delay(400);

  //V

  digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, HIGH);   
  delay(600); 
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
   digitalWrite(LED_BUILTIN, LOW);   
  delay(400);

  //B

   digitalWrite(LED_BUILTIN, HIGH);   
  delay(600); 
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
   digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);
  digitalWrite(LED_BUILTIN, HIGH);   
  delay(200);                       
  digitalWrite(LED_BUILTIN, LOW);   
  delay(200);

 digitalWrite(LED_BUILTIN, LOW);   
  delay(1000);
}


